N1 = float(input("Enter your first number :"))
N2 = float(input("Enter your second number :"))

if N2 < N1:
        percent_diff = ((N1 - N2)/N1) * 100
        print("Difference in percentage is : -{}%".format(percent_diff))
else:
	percent_diff = ((N2 - N1)/N1) * 100

	print("Difference in percentage is : {}%".format(percent_diff))
